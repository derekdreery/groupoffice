GO.admin2userlogin.UsersGrid=function(b){if(!b){b={}}var a={fields:["id","username","name","lastlogin","registration_time"],columns:[{header:GO.admin2userlogin.lang.userId,dataIndex:"id",id:"id",width:60},{header:GO.admin2userlogin.lang.username,dataIndex:"username",id:"username",width:180},{header:GO.admin2userlogin.lang.name,dataIndex:"name",id:"name",width:100},{header:GO.admin2userlogin.lang.lastlogin,dataIndex:"lastlogin",id:"lastlogin",width:110},{header:GO.admin2userlogin.lang.registrationtime,dataIndex:"registration_time",id:"registration_time",width:110}]};this.store=new GO.data.JsonStore({url:GO.settings.modules.admin2userlogin.url+"json.php",baseParams:{task:"usersgrid"},root:"results",id:"id",totalProperty:"total",fields:a.fields,remoteSort:true});this.searchField=new GO.form.SearchField({store:this.store,width:320});b.title=GO.admin2userlogin.lang.admin2userlogin;b.layout="fit";b.autoScroll=true;b.split=true;b.autoExpandColumn="name";b.store=this.store;b.paging=true;var c=new Ext.grid.ColumnModel({defaults:{sortable:true},columns:a.columns});b.cm=c;b.view=new Ext.grid.GridView({emptyText:GO.lang.strNoItems});b.sm=new Ext.grid.RowSelectionModel();b.tbar=new Ext.Toolbar({items:[GO.lang.strSearch+": "," ",this.searchField],cls:"go-head-tb"});b.loadMask=true;Ext.apply(b,{listeners:{render:function(){b.store.load()}}});GO.admin2userlogin.UsersGrid.superclass.constructor.call(this,b);this.on("rowdblclick",function(d,f){var e=d.getStore().getAt(f).data;document.location=GO.settings.modules.admin2userlogin.url+"changeUser.php?id="+e.id},this)};Ext.extend(GO.admin2userlogin.UsersGrid,GO.grid.GridPanel,{});GO.moduleManager.addModule("admin2userlogin",GO.admin2userlogin.UsersGrid,{title:GO.admin2userlogin.lang.admin2userlogin,iconCls:"go-tab-icon-admin2userlogin",admin:true});